{
  "name": "react-template",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "electron": "electron .",
  "description": "React项目最佳模板",
  "author": {
    "name": "Rhine-AI-Lab"
  },
  "scripts": {
    "install": "yarn install",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "start:qa": "cross-env NODE_ENV=qa react-app-rewired start",
    "dev": "concurrently \"electron .\" \"cross-env BROWSER=none yarn start\"",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "electron": "electron .",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --fix",
    "check": "eslint --ext .js,.jsx,.ts,.tsx src",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\""
  },
  "dependencies": {
    "@babel/core": "^7.22.20",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@material/web": "^1.0.0",
    "@mui/material": "^5.14.10",
    "@mui/styled-engine-sc": "^5.14.10",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@reduxjs/toolkit": "^1.9.5",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.3",
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "axios": "^1.5.0",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-named-asset-import": "^0.3.8",
    "babel-preset-react-app": "^10.0.1",
    "bfj": "^8.0.0",
    "browserslist": "^4.21.10",
    "camelcase": "^8.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "customize-cra": "^1.0.0",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "electron-is-dev": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.4",
    "eslint": "^8.49.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.1.1",
    "html-webpack-plugin": "^5.5.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-resolve": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "mini-css-extract-plugin": "^2.7.6",
    "node-notifier": "^10.0.1",
    "notistack": "^3.0.1",
    "postcss": "^8.4.30",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.3.3",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.1.4",
    "prompts": "^2.4.2",
    "react": "^18.2.0",
    "react-app-polyfill": "^3.0.0",
    "react-app-rewired": "^2.2.1",
    "react-dev-utils": "^12.0.1",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.16.0",
    "react-scripts": "^5.0.1",
    "resolve": "^1.22.6",
    "resolve-url-loader": "^5.0.0",
    "sass-resources-loader": "^2.2.5",
    "semver": "^7.5.4",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.3",
    "styled-components": "^6.0.8",
    "tailwindcss": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.2.2",
    "web-vitals": "^3.4.0",
    "webpack": "^5.88.2",
    "webpack-dev-server": "^4.15.1",
    "webpack-manifest-plugin": "^5.0.0",
    "workbox-webpack-plugin": "^7.0.0"
  },
  "config-overrides-path": "config/configOverrides.js",
  "build": {
    "appId": "ReactTemplate",
    "mac": {
      "category": "public.app-category.developer-tools"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.4.2",
    "@electron-forge/maker-deb": "^6.4.2",
    "@electron-forge/maker-rpm": "^6.4.2",
    "@electron-forge/maker-squirrel": "^6.4.2",
    "@electron-forge/maker-zip": "^6.4.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "electron": "^26.2.2",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "redux-devtools": "^3.7.0",
    "sass": "^1.68.0",
    "sass-loader": "^13.3.2",
    "wait-on": "^7.0.1"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "electronPackagerConfig": {
        "packageManger": "yarn",
        "asar": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "iot_simulator"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
